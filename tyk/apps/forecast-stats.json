{
  "name": "Forecast Stats API",
  "api_id": "forecast-stats",
  "org_id": "default",
  "definition": {
    "location": "header",
    "key": "x-api-key"
  },
  "auth": {
    "auth_header_name": "authorization"
  },
  "version_data": {
    "not_versioned": true,
    "versions": {
      "Default": {
        "name": "Default",
        "use_extended_paths": true
      }
    }
  },
  "proxy": {
    "listen_path": "/api/forecast/stats",
    "target_url": "http://forecast-stats:8081/forecast/stats",
    "strip_listen_path": true
  },
  "CORS": {
    "enable": true,
    "allowed_origins": [
      "http://localhost:5173",
      "http://127.0.0.1:5173"
    ],
    "allowed_methods": [
      "GET"
    ],
    "allowed_headers": [
      "Accept",
      "Content-Type",
      "Content-Length",
      "Accept-Encoding",
      "X-CSRF-Token",
      "Authorization",
      "X-Requested-With"
    ],
    "allow_credentials": true,
    "options_passthrough": false,
    "debug_allow_all_origins": false
  },
  "use_keyless": true,
  "global_rate_limit": {
    "rate": 300,
    "per": 60
  },
  "active": true
}
